{
    "name": "root",
    "version": "1.0.0",
    "private": true,
    "license": "UNLICENSED",
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "doctor": "yarn dlx @yarnpkg/doctor",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css,less}\" --ignore-path .gitignore",
        "lint": "eslint --ignore-path '.eslintignore' '{apps,packages}/**/*'",
        "typecheck": "yarn workspaces foreach -ptv --exclude root run typecheck"
    },
    "dependencies": {
        "@emotion/react": "11.1.4",
        "@emotion/styled": "11.0.0",
        "@material-ui/core": "next",
        "@yarnpkg/pnpify": "2.4.0",
        "next": "10.0.3",
        "react": "17.0.1",
        "react-dom": "17.0.1"
    },
    "devDependencies": {
        "@types/eslint": "7.2.6",
        "@types/eslint-plugin-prettier": "3.1.0",
        "@types/node": "12.19.9",
        "@types/react": "17.0.0",
        "@types/react-dom": "17.0.0",
        "@typescript-eslint/eslint-plugin": "4.11.0",
        "@typescript-eslint/parser": "4.11.0",
        "babel-eslint": "10.1.0",
        "eslint": "7.16.0",
        "eslint-config-airbnb": "18.2.1",
        "eslint-config-prettier": "7.1.0",
        "eslint-config-react-app": "6.0.0",
        "eslint-plugin-flowtype": "5.2.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-jsx-a11y": "6.4.1",
        "eslint-plugin-prettier": "3.3.0",
        "eslint-plugin-react": "7.21.5",
        "eslint-plugin-react-hooks": "4.2.0",
        "husky": "4.3.6",
        "prettier": "2.2.1",
        "prettier-plugin-packagejson": "2.2.8",
        "typescript": "4.0"
    }
}
